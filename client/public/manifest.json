{
  "manifest_version": 3,
  "name": "Video Downloader",
  "version": "1.0.0",
  "description": "Download YouTube videos with ease",
  "icons": {
    "16": "download.png",
    "48": "download.png",
    "128": "download.png"
  },
  "action": {
    "default_icon": "download.png",
    "default_popup": "index.html",
    "default_title": "Video Downloader"
  },
  "permissions": [
    "activeTab",
    "storage"
  ]
}
